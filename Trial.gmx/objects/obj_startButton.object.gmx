if ((mouse_x  != obj_startButton.x) &amp;&amp; (mouse_y != obj_startButton.y) &amp;&amp; (mouse_check_button_released(mb_left) == true)){
    image_index = 0;//Make the image index 0 (AKA, the not pressed frame)
};
